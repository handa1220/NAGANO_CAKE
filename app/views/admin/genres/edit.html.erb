<header>
    <%= render "admin/shared/header" %>
</header>

<main class="border">
  <span class="h4 bg-light">ジャンル編集</span>

  <%= form_with model: @genre, url: admin_genre_path(@genre.id), method: :patch do |f| %>
  <div class=field>
    <%= f.label :name, "ジャンル名" %>
    <%= f.text_field :name %>
    <%= f.submit "変更を保存", class: "btn btn-success" %>
  </div>
  <% end %>



</main>
